<% include ../partials/header %>

<div class="ui container">
  <div class="ui header centered huge"><%= title %></div>
  <div class="ui link cards centered">
    <% skills.forEach(function(e) { %>
      <div class="card">
        <div class="image">
          <img src="<%= e.imagePath %>" alt="">
        </div>
        <div class="content">
          <div class="header"><%= e.title %></div>
          <div class="meta">
          </div>
          <div class="description">
            <%= e.description %>
          </div>
        </div>
        <div class="extra content">
          <span class="price right floated">
            <%= e.price %>
          </span>
          <span class="ui button inverted green">
            Buy
          </span>
        </div>
      </div>
      <% }) %>
    </div>
  </div>

  <% include ../partials/footer %>
